{
  "name" : "wfileswatcher",
  "version" : "0.1.19",
  "engine" : "node >= 10.0.0",
  "enabled" : 1,
  "description" : "Aggregates file watching strategies and provides a single interface to them. The strategy can be a wrapper around a built-in feature or a separate library. Use the module to easily create watcher instances, pause/resume watchers, switch between watcher strategies and control created watchers using the watcher manager class.",
  "license" : "MIT",
  "author" : "Kostiantyn Wandalen <wanalen@01.school>",
  "contributors" :
  [
    { "name" : "Kostiantyn Wandalen", "email" : "wanalen@01.school" },
    { "name" : "Volodymyr M.", "email" : "volodymyr.migdal@gmail.com" }
  ],
  "main" : "proto/node_modules/wFilesWatcher",
  "files" : [ "proto/wtools/amid/l4_files/l7_watcher", "out", "proto/node_modules/Tools", "proto/node_modules/wFilesWatcher" ],
  "scripts" :
  {
    "test" : "wtest .run 'proto/**' rapidity:-1",
    "docgen" : "wdocgen proto",
    "watches.limit.get" : "node proto/wtools/amid/l4_files/l7_watcher/step/WatchesLimit.s get",
    "watches.limit.set" : "node proto/wtools/amid/l4_files/l7_watcher/step/WatchesLimit.s set"
  },
  "repository" :
  {
    "type" : "git",
    "url" : "https://github.com/Wandalen/wFilesWatcher.git"
  },
  "bugs" :
  {
    "url" : "https://github.com/Wandalen/wFilesWatcher/issues"
  },
  "dependencies" :
  {
    "fb-watchman" : "2.0.1",
    "wTools" : "gamma",
    "wConsequence" : "gamma",
    "wCopyable" : "gamma",
    "wProto" : "gamma",
    "wEventHandler" : "gamma",
    "wFiles" : "gamma"
  },
  "devDependencies" : { "eslint" : "7.1.0", "wTesting" : "gamma" },
  "keywords" : [ "wTools", "wFilesWatcher", "fileswatcher" ]
}
